                    <filter name="group_company_owner" string="Company Owner" context="{'group_by': 'company_owner'}"/>
                    <filter name="group_contact_owner" string="Contact Owner" context="{'group_by': 'contact_owner'}"/>                    
                </field>
            </field>
        </record>

