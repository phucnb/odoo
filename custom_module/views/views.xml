		 <!-- ADD CITY TO COMPANY CARD ON KANBAN VIEW -->
		<record id="custom_module_res_partner_inherit_kanban_view" model="ir.ui.view">
            <field name="name">custom.module.res.partner.inherit.kanban.view</field>
            <field name="model">res.partner</field>
            <field name="inherit_id" ref="base.res_partner_kanban_view"/>
            <field name="priority" eval="20"/>
            <field name="arch" type="xml">
                <xpath expr="//div[hasclass('flex-column')]/ul/li/t/field[@name='city']" position="after">
                    <t t-if="record.state_id.raw_value">, </t>
                    <t t-if="record.state_id.raw_value"><field name="state_id"/></t>
                    <!-- <t t-if="record.job_title.raw_value"><field name="job_title"/></t> -->
                </xpath>
               <xpath expr="//div[hasclass('flex-column')]/ul/li/field[@name='parent_id']" position="replace">
