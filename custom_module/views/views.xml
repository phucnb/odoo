					<field attrs="{'invisible':[('is_company','=',True)]}" name="first_name"/>
					<field attrs="{'invisible':[('is_company','=',True)]}" name="last_name"/>
					<field attrs="{'invisible':[('is_company','=',True)]}" name="title" options="{'no_create': True, 'no_edit': True, 'no_create_edit': True}"/>
					<field attrs="{'invisible':[('is_company','=',True)]}" name="job_title" placeholder="Director of Transportation"/>
