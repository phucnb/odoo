                           options="{'no_create': True, 'no_edit': True, 'no_create_edit': True}"
                           domain="['&amp;',('type','=','contact'),('id','in',child_ids)]"
                           attrs="{'invisible': [('is_company','=',True)]}"/>

				</field>
				<!-- RIGHT SIDE CONTACT CARD -->
				<field name="category_id" position="before">
					<field attrs="{'invisible':[('is_company','=',True)]}" name="contact_owner" options="{'no_create': True, 'no_edit': True, 'no_create_edit': True}" widget="many2one_avatar_user"/>
